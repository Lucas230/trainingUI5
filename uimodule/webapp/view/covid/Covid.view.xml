 <mvc:View controllerName="com.lp.trainingUI5.controller.covid.Covid"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:viz="sap.viz.ui5.controls"
  xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
  xmlns:viz.data="sap.viz.ui5.data" height="100%">

  <Page id="covid" showNavButton="true" navButtonPress="onNavBack">
    <Panel
        expandable="true"
        expanded="true"
        headerText="Data"
        width="auto"
        class="panelStyle">

        <content>
            <Button text="Latest Count by List"
              press="onPressList"
              ariaDescribedBy="defaultButtonDescription genericButtonDescription"/>
            <Button text="Latest Counter by Graph"
              press="onPressGraph"
              ariaDescribedBy="defaultButtonDescription genericButtonDescription"/>
        </content>
    </Panel>
    <viz:Popover id="idPopOver"></viz:Popover>
    <viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}"
        height='100%' width="100%" vizType='line'>
        <viz:dataset>
            <viz.data:FlattenedDataset data="{/data}">
                <viz.data:dimensions>
                    <viz.data:DimensionDefinition name="day"
                        value="{day}" />
                </viz.data:dimensions>
                <viz.data:measures>
                    <viz.data:MeasureDefinition name="total"
                        value="{summary/total}" />
                </viz.data:measures>
            </viz.data:FlattenedDataset>
        </viz:dataset>

        <viz:feeds>
            <viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure"
                values="total" />
            <viz.feeds:FeedItem id='categoryAxisFeed' uid="categoryAxis" type="Dimension"
                values="day" />
        </viz:feeds>
    </viz:VizFrame>
  </Page>
</mvc:View>